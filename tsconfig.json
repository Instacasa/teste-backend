{
  "compilerOptions": {
    "strict": true,
    "strictPropertyInitialization": false,
    "strictBindCallApply": false,
    "strictNullChecks": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "noImplicitAny": false,
    "baseUrl": ".",
    "types": ["node", "jest"],
    "paths": {
      "@controllers": ["src/infra/http/controllers"],
      "@routes": ["src/infra/http/routes"],
      "@serializers/*": ["src/interfaces/http/serializers/*"],
      "@middleware/*": ["lib/interfaces/http/middleware/*"],
      "@models/*": ["src/infra/database/models/*"],
      "@database": ["src/infra/database"],
      "@database/*": ["src/infra/database/*"],
      "@infraHelpers/*": ["src/infra/helpers/*"],
      "@auth0Management/*": ["src/infra/auth0Management/*"],
      "@auth0Management": ["src/infra/auth0Management"],
      "@repositories/*": ["src/infra/database/repositories/*"],
      "@enumerators/*": ["src/infra/enumerators/*"],
      "@domains/*": ["src/domains/*"],
      "@apps/*": ["src/app/*"],
      "@libs/*": ["lib/*"],
      "@config": ["config"],
      "@types": ["src/types.ts"],
      "@constants": ["src/constants"],
      "@/*": ["src/*"]
    }
  },
  "lib": ["es6"]
}
